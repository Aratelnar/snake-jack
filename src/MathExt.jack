class MathExt {
    function int mod(int a, int b){
        var int d;
        let a = Math.abs(a);
        let b = Math.abs(b);
        if (a < b){
            return a;
        }
        if (a = b){
            return 0;
        }
        let d = Math.divide(a, b);
        return a - (b * d);
    }
}
